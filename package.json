{
  "name": "enceladus",
  "description": "enceladus",
  "repository": {
    "type": "git",
    "url": "https://github.com/cblanc/enceladus.git"
  },
  "bugs": {
    "url": "https://github.com/cblanc/enceladus/issues/new"
  },
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "npm run lint && NODE_ENV=test nyc mocha",
    "lint": "eslint lib/**/*.ts",
    "build": "npm run lint && tsc",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "release": "semantic-release --no-ci",
    "prepublishOnly": "npm test && npm run build",
    "watch": "mocha --watch-extensions ts --watch"
  },
  "prettier": {
    "editorconfig": true,
    "trailingComma": "es5"
  },
  "mocha": {
    "extension": [
      "ts"
    ],
    "exit": true,
    "fullTrace": true,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "nyc": {
    "include": [
      "lib/**/*.ts"
    ],
    "exclude": [
      "dist/*",
      "**/*.d.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true
  },
  "engines": {
    "node": ">=8.0"
  },
  "author": {
    "name": "Christopher Blanchard",
    "email": "git@nf3.co",
    "url": "nf3.co"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "release": {
    "extends": "@cablanchard/semantic-release"
  },
  "devDependencies": {
    "@cablanchard/eslint-config": "~2.0.2",
    "@cablanchard/semantic-release": "~1.3.0",
    "@cablanchard/tsconfig": "~1.0.0",
    "@types/chai": "~4.2.0",
    "@types/mocha": "~7.0.1",
    "@typescript-eslint/eslint-plugin": "~3.1.0",
    "chai": "~4.2.0",
    "codecov": "~3.7.0",
    "eslint": "~7.2.0",
    "mocha": "~7.2.0",
    "nyc": "~15.1.0",
    "prettier": "~2.0.1",
    "semantic-release": "~17.0.7",
    "source-map-support": "~0.5.9",
    "ts-node": "~8.10.1",
    "tslint": "~6.1.0",
    "typescript": "~3.9.2"
  },
  "dependencies": {}
}
