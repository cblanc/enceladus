{
  "name": "enceladus",
  "version": "0.0.1",
  "description": "enceladus",
  "repository": {
    "type": "git",
    "url": "git@github.com:cblanc/enceladus"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "npm run lint && NODE_ENV=test node_modules/.bin/nyc node_modules/.bin/mocha --opts mocha.opts",
    "lint": "node_modules/.bin/tslint -c tslint.json -p tsconfig.json lib/**/*.ts",
    "build": "node_modules/.bin/parcel build ./html/index.html --public-url '/'",
    "coverage": "node_modules/.bin/nyc report --reporter=text-lcov | node_modules/.bin/coveralls || true",
    "start": "node_modules/.bin/parcel ./html/index.html"
  },
  "nyc": {
    "include": [
      "lib/**/*.ts"
    ],
    "exclude": [
      "dist/*"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
  },
  "engines": {
    "node": ">=6.9"
  },
  "author": "cblanc",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "~4.1.3",
    "@types/mocha": "~5.2.0",
    "chai": "~4.1.2",
    "coveralls": "~3.0.1",
    "mocha": "~5.2.0",
    "nyc": "~12.0.1",
    "source-map-support": "~0.5.6",
    "ts-node": "~6.1.0",
    "tslint": "~5.10.0",
    "typings": "~2.1.1"
  },
  "dependencies": {
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "parcel-bundler": "^1.8.1",
    "parcel-plugin-typescript": "^0.7.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "tslint-react": "^3.6.0",
    "typescript": "~2.9.1"
  }
}
